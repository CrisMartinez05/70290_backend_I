<h1>"Vivir Fitness - Lista de productos:"</h1>
  
  <div>
    {{#if products}}
      {{#each products}}
      <div>
        <p>Categoría: <b>{{this.category}}</b>  - Cód: <b>{{this.code}}</b></p>
        <h3>Id: {{this.id}} - {{this.title}}</h3>
        <p>{{this.description}}</p>
        <p>Stock: <b>{{this.stock}}</b> </p>
        <p>Precio:<b> $ {{this.price}}</b></p>
        <hr />
      </div>
      {{/each}}
    {{else}}
      <p>No hay productos para mostrar</p>
    {{/if}}
  </div>
